   "source": [
    "n = int(input())\n",
    "rope = []\n",
    "for _ in range(n):\n",
    "    rope.append(int(input()))\n",
    "\n",
    "rope.sort()\n",
    "res = rope[0] *n\n",
    "\n",
    "k = n\n",
    "for i in range(1,n):\n",
    "    k = k-1\n",
    "    if rope[i] != rope[i-1]:\n",
    "        if res <= rope[i] * k:\n",
    "            res = rope[i] *k\n",
    "    \n",
    "print(res)"
   ]
