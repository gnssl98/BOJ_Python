    "import math\n",
    "\n",
    "N = int(input())\n",
    "arr=list(map(int, input().split()))\n",
    "X = int(input())\n",
    "res =[]\n",
    "\n",
    "for i in range(N):\n",
    "    if math.gcd(arr[i],X) ==1:\n",
    "        res.append(arr[i])\n",
    "\n",
    "mean = sum(res) / len(res)\n",
    "print(f\"{mean:.6f}\")"
   ]
  }
