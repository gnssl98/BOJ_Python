    "N = int(input())\n",
    "fact = 1\n",
    "cnt=0\n",
    "for i in range(1,N+1):\n",
    "    fact = fact * i\n",
    "\n",
    "    \n",
    "for j in reversed(range(len(str(fact)))):\n",
    "    if str(fact)[j] == '0':\n",
    "        cnt +=1\n",
    "    else :\n",
    "        break\n",
    "        \n",
    "print(cnt)\n",
    "        \n"
   ]
